{
  "name": "members-search-api",
  "version": "0.0.1",
  "description": "API service for Members Search",
  "main": "handler.js",
  "scripts": {
    "start": "SLS_DEBUG=* sls offline start",
    "db:start": "sls dynamodb start --migrate",
    "db:install": "sls dynamodb install",
    "db:fix": "kill $(lsof -i tcp:8000 | awk 'FNR == 2 {print $2}')",
    "build": "NODE_ENV=production sls package --stage=production",
    "test:lint": "eslint .",
    "test:unit": "NODE_ENV=test ava tests/unit/**/*.test.js",
    "test:integration": "NODE_ENV=test ava --serial --fail-fast tests/integration/**/*.test.js",
    "test:deploy": "NODE_ENV=production sls deploy --stage=${DEPLOY_STAGE:-development} --no-deploy ",
    "deploy": "SLS_DEBUG=* NODE_ENV=production sls deploy --stage=${DEPLOY_STAGE:-development}",
    "deploy:production": "NODE_ENV=production sls deploy --stage=production",
    "logs": "sls logs --function=${LOGS_FUNCTION:-graphql} --stage=${LOGS_STAGE:-development} --tail",
    "copy:graphql": "find src -name \\*.graphql -exec cp {} ./dist/{} \\;",
    "package": "SLS_DEBUG=* sls package",
    "copy:members-schema": "cp graphql/members/members-schema.graphql graphql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escabc/members-search-api.git"
  },
  "author": "Viatcheslav Ivanov <slava@makkke.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/escabc/members-search-api/issues"
  },
  "homepage": "https://github.com/escabc/members-search-api#readme",
  "ava": {
    "require": [
      "@babel/register"
    ],
    "babel": true
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@graphql-tools/schema": "^8.3.0",
    "babel-polyfill": "^6.23.0",
    "camelcase-keys": "7.0.1",
    "csv-parse": "4.16.3",
    "dotenv": "10.0.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "graphql": "15.6.1",
    "lodash": "^4.17.11",
    "moment": "^2.18.1",
    "node-fetch": "3.0.0",
    "path": "^0.12.7",
    "serverless-dynamodb-client": "0.0.2",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "ava": "3.15.0",
    "babel-plugin-add-module-exports": "1.0.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "8.0.1",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "serverless": "2.62.0",
    "serverless-bundle": "^5.0.2",
    "serverless-copy-webpack-plugin": "^1.1.0",
    "serverless-dynamodb-local": "0.2.40",
    "serverless-offline": "8.2.0",
    "serverless-webpack": "^5.5.5"
  }
}
